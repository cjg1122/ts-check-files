{
  "name": "ts-check-files",
  "version": "2.0.1",
  "description": "typescript check files",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "tscf": "dist/bin.js",
    "ts-check-files": "dist/bin.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "check": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true",
    "prepublishOnly": "npm run build"
  },
  "files": ["dist"],
  "keywords": [
    "check type",
    "tsc",
    "ts check",
    "tsc files",
    "tscf",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cjg1122/ts-check-files"
  },
  "author": {
    "name": "cjg",
    "url": "https://github.com/cjg1122"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cjg1122/ts-check-files/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "^22.7.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
